        - <span style="color:#f92672">node</span><span style="color:#111">:</span> <span style="color:#ae81ff">node-sm</span>
          <span style="color:#f92672">service</span><span style="color:#111">:</span> <span style="color:#ae81ff">supervisor</span>
          <span style="color:#f92672">label</span><span style="color:#111">:</span> <span style="color:#ae81ff">supervisor</span>
          <span style="color:#f92672">replicas</span><span style="color:#111">:</span> <span style="color:#ae81ff">2</span>
          <span style="color:#f92672">networks</span><span style="color:#111">:</span>
            - <span style="color:#f92672">name</span><span style="color:#111">:</span> <span style="color:#ae81ff">internet</span>
              <span style="color:#f92672">uplink</span><span style="color:#111">:</span>
                <span style="color:#f92672">bandwidth</span><span style="color:#111">:</span> <span style="color:#ae81ff">1Mbps</span>
                <span style="color:#f92672">latency</span><span style="color:#111">:</span>
                  <span style="color:#f92672">delay</span><span style="color:#111">:</span> <span style="color:#ae81ff">200ms</span>
                <span style="color:#f92672">drop</span><span style="color:#111">:</span> <span style="color:#ae81ff">0.01</span><span style="color:#ae81ff">%</span>
              <span style="color:#f92672">downlink</span><span style="color:#111">:</span>
                <span style="color:#f92672">bandwidth</span><span style="color:#111">:</span> <span style="color:#ae81ff">1Mbps</span>
                <span style="color:#f92672">latency</span><span style="color:#111">:</span>
                  <span style="color:#f92672">delay</span><span style="color:#111">:</span> <span style="color:#ae81ff">200ms</span>
